        <param name="conversionPattern" value="时间：%d%n线程：[%t]%n类型：%-5p%n标题：%c%n内容：%m%n——————————%n" />
        <param name="Header" value="&#xD;&#xA;----------------------header--------------------------&#xD;&#xA;&#xD;&#xA;" />
        <param name="Footer" value="&#xD;&#xA;----------------------footer--------------------------&#xD;&#xA;" />
      </layout>
    </appender>
